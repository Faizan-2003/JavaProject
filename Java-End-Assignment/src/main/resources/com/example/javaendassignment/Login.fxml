<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<VBox prefHeight="256.0" prefWidth="359.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.javaendassignment.Controllers.LoginController">
   <Label prefHeight="62.0" prefWidth="72.0" text="Login">
      <font>
         <Font name="System Bold" size="26.0" />
      </font>
      <VBox.margin>
         <Insets bottom="-20.0" left="18.0" />
      </VBox.margin>
   </Label>
    <GridPane hgap="30.0" prefHeight="192.0" prefWidth="286.0">
        <columnConstraints>
            <ColumnConstraints hgrow="NEVER" minWidth="10.0" prefWidth="100.0" />
            <ColumnConstraints hgrow="NEVER" minWidth="10.0" prefWidth="150.0" />
        </columnConstraints>
        <rowConstraints>
            <RowConstraints minHeight="10.0" prefHeight="40.0" />
            <RowConstraints minHeight="10.0" prefHeight="40.0" vgrow="NEVER" />
            <RowConstraints minHeight="10.0" prefHeight="50.0" vgrow="NEVER" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="NEVER" />
         <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="NEVER" />
        </rowConstraints>
        <children>
            <Label text="Username">
            <font>
               <Font size="18.0" />
            </font></Label>
            <Label text="Password" GridPane.rowIndex="1">
            <font>
               <Font size="18.0" />
            </font></Label>
            <TextField fx:id="UsernameField" GridPane.columnIndex="1" />
            <PasswordField fx:id="passwordField" onInputMethodTextChanged="#onPasswordTextChange" prefHeight="25.0" prefWidth="150.0" GridPane.columnIndex="1" GridPane.rowIndex="1" />

            <Button fx:id="loginButton" onAction="#onLoginButtonClick" prefHeight="38.0" prefWidth="74.0" style="-fx-background-color: #AEEEEE;" text="Login" wrapText="true" GridPane.columnIndex="1" GridPane.rowIndex="2">
            <font>
               <Font size="14.0" />
            </font></Button>
        </children>
      <VBox.margin>
         <Insets left="18.0" top="27.0" />
      </VBox.margin>
      <opaqueInsets>
         <Insets top="23.0" />
      </opaqueInsets>
    </GridPane>
    <Label fx:id="ErrorMessage" prefHeight="17.0" prefWidth="13.0" text="${passwordField.text}" GridPane.rowIndex="3">
      <VBox.margin>
         <Insets top="-12.0" />
      </VBox.margin></Label>
    <padding>
        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
    </padding>
</VBox>
